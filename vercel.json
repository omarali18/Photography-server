{
    "version":2,
    "builds": [
        {
            "src": "package.json",
            "use": "@versel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem",
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
        }
    ]
}